    "@types/node": "^22.15.3",
    "dotenv-cli": "^7.4.4",
    "typescript": "5.8.2"
  },
  "eslintConfig": {
    "extends": [
      "@shortcart-v3/eslint-config/library"
    ]
  },
  "prettier": "@shortcart-v3/prettier"
